{
  "version": 2,
  "buildCommand": "npm install || true",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 60
    },
    "api/vercel.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "env": {
    "DB_TYPE": "postgresql",
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel.js"
    }
  ]
}